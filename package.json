    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "jest": "^21.2.1",
    "prettier": "^1.9.2",
    "semantic-release": "^8.2.0"
  },
  "dependencies": {
    "eslint-module-utils": "^2.1.1",
    "globby": "^7.1.1",
